<script>
  "use strict";

  document.addEventListener('DOMContentLoaded', () => {
    const setActive = (active, inactive) => {
      active.classList.remove('bg-transparent', 'text-white')
      active.classList.add('bg-white', 'text-gray-800')
      inactive.classList.remove('bg-white', 'text-gray-800')
      inactive.classList.add('bg-transparent', 'text-white')
    }

    const monthlyToggle = document.querySelector('#js-monthly-toggle');
    const yearlyToggle  = document.querySelector('#js-yearly-toggle');

    const priceLite = document.querySelector('#js-price-lite');
    const priceStarter = document.querySelector('#js-price-starter');
    const pricePro = document.querySelector('#js-price-pro');
    const interval = document.querySelectorAll('#js-interval');

    monthlyToggle.addEventListener('click', () => {
      priceLite.innerHTML = "$6"
      priceStarter.innerHTML = "$10"
      pricePro.innerHTML = "$28"
      interval.forEach(text => {
        text.textContent = "per month";
      });
      setActive(monthlyToggle, yearlyToggle);
    });

    yearlyToggle.addEventListener('click', () => {
      priceLite.innerHTML = "$59"
      priceStarter.innerHTML = "$99"
      pricePro.innerHTML = "$279"
      interval.forEach(text => {
        text.textContent = "per year";
      });
      setActive(yearlyToggle, monthlyToggle);
    });
  })
</script>

