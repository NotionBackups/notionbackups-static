---
title: Automatically Back Up and Restore your Notion Data
description: Easily back up your Notion workspaces to Google Drive, Microsoft OneDrive, Dropbox, Amazon S3, and Backblaze B2. Restore to a specific point in time with a few clicks.
---

<% content_for :head do %>
  <script type="application/ld+json">
  {
    "@context": "https://schema.org/",
    "@type": "Product",
    "name": "Notion Backups",
    "description": "<%= @item[:description] %>",
    "image": "https://notionbackups.com/assets/images/banner.png",
    "brand": {
      "@type": "Brand",
      "name": "Notion Backups"
    },
    "offers": {
      "@type": "Offer",
      "price": "10",
      "priceCurrency": "USD",
      "hasMerchantReturnPolicy": {
        "@type": "MerchantReturnPolicy",
        "returnFees": "https://schema.org/FreeReturn"
      }
    }
  }
  </script>
<% end %>

<script>
  "use strict";

  document.addEventListener('DOMContentLoaded', () => {
    const monthlyToggle = document.querySelector('#js-monthly-toggle');
    const yearlyToggle  = document.querySelector('#js-yearly-toggle');

    const price = document.querySelector('#js-price');
    const interval = document.querySelector('#js-interval');

    monthlyToggle.addEventListener('click', () => {
      price.innerHTML = "$10"
      interval.textContent = "per month";
    });

    yearlyToggle.addEventListener('click', () => {
      price.innerHTML = "$99"
      interval.textContent = "per year";
    });
  })
</script>

